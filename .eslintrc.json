{
    "env": {
        "es2022": true,
        "node": true
    },
    "plugins": [
        "prettier"
    ],
    "extends": [
        "airbnb-base", 
        "prettier"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "no-plusplus": "off",
        "import/extensions": [
            "error",
            "ignorePackages",
            {
              "js": "always"
            }
          ]
    }
}